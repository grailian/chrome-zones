{
  "name": "chrome-zones",
  "version": "0.1.0",
  "LICENSE": "MIT",
  "scripts": {
    "buildMain": "parcel build src/index.js -d ./build/ -o main.js",
    "watchMain": "parcel watch src/index.js -d ./build/ -o main.js --no-hmr",
    "buildBackground": "parcel build src/background.js -d ./build/ -o background.js",
    "watchBackground": "parcel watch src/background.js -d ./build/ -o background.js --no-hmr"
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "prop-types": "15.7.2",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "typeface-montserrat": "0.0.54",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-runtime": "6.26.0",
    "parcel-bundler": "1.11.0",
    "parcel-plugin-static-files-copy": "2.0.0"
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
